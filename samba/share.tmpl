[${VOLUME_NAME}]
  comment = ${VOLUME_NAME} volume from ${CONTAINER}
  read only = ${READONLY}
  available = yes
  public = yes
  valid users = ${USER},root,guest
  browsable = yes
  locking = no
  path = ${VOLUME}
  force user = ${USER}
  force group = ${GROUP}
  guest ok = yes
  map archive = no
  map system = no
  map hidden = no
